{
  "version": 1,
  "canisters": {
    "vault": {
      "type": "custom",
      "candid": "src/vault/vault.did",
      "wasm": "vault.wasm",
      "build": "src/vault/build.sh",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    },
    "ledger": {
      "type": "custom",
      "candid": "ledger.did",
      "wasm": "ledger.wasm"
    },
    "pool_stats": {
      "type": "custom",
      "candid": "src/pool_stats/pool_stats.did",
      "wasm": "pool_stats.wasm",
      "build": "src/pool_stats/build.sh",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    }
  },
  "defaults": {
    "replica": {
      "subnet_type": "system"
    },
    "build": {
      "packtool": "",
      "args": ""
    }
  },
  "networks": {
    "dev": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "staging": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "ic": {
      "providers": [
        "https://icp0.io"
      ],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    }
  }
}